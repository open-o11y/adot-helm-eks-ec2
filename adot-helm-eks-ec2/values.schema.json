{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "nameOverride": {
            "type": "string"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "namespaceOverride": {
                    "type": "string"
                }
            }
        },
        "awsRegion": {
            "type": "string"
        },
        "clusterName": {
            "type": "string"
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                },
                "annotations": {
                    "type": "object"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "fluentbit": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "namespaceOverride": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "configName": {
                    "type": "string"
                },
                "namespace": {
                    "type": "string"
                },
                "image": {
                    "type": "object",
                    "required": [
                        "repository",
                        "pullPolicy"
                    ],
                    "properties": {
                        "repository": {
                            "type": "string",
                            "pattern": "^[/a-z0-9-_]+$"
                        },
                        "tag": {
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string",
                            "pattern": "^(Always|Never|IfNotPresent)$"
                        }
                    }
                },
                "input": {
                    "type": "object",
                    "properties": {
                        "applicationLog": {
                            "type": "object",
                            "properties": {
                                "tag": {
                                    "type": "string"
                                },
                                "excludePath": {
                                    "type": "string"
                                },
                                "path": {
                                    "type": "string"
                                },
                                "dockerMode": {
                                    "type": "string"
                                },
                                "dockerModeFlush": {
                                    "type": "integer"
                                },
                                "parser": {
                                    "type": "string"
                                },
                                "dockerModeParser": {
                                    "type": "string"
                                },
                                "db": {
                                    "type": "string"
                                },
                                "memBufLimit": {
                                    "type": "string"
                                },
                                "skipLongLines": {
                                    "type": "string"
                                },
                                "refreshInterval": {
                                    "type": "integer"
                                },
                                "rotateWait": {
                                    "type": "integer"
                                },
                                "storageType": {
                                    "type": "string"
                                },
                                "fluentbit": {
                                    "type": "object",
                                    "properties": {
                                        "path": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        },
                                        "memBufLimit": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "cloudwatchAgent": {
                                    "type": "object",
                                    "properties": {
                                        "path": {
                                            "type": "string"
                                        },
                                        "dockerModeParser": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        },
                                        "memBufLimit": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        },
                        "dataplaneLog": {
                            "type": "object",
                            "properties": {
                                "systemd": {
                                    "type": "object",
                                    "properties": {
                                        "tag": {
                                            "type": "string"
                                        },
                                        "dockerUnit": {
                                            "type": "string"
                                        },
                                        "kubeletUnit": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        },
                                        "path": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "tail": {
                                    "type": "object",
                                    "properties": {
                                        "tag": {
                                            "type": "string"
                                        },
                                        "path": {
                                            "type": "string"
                                        },
                                        "dockerMode": {
                                            "type": "string"
                                        },
                                        "dockerModeFlush": {
                                            "type": "integer"
                                        },
                                        "parser": {
                                            "type": "string"
                                        },
                                        "dockerModeParser": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        },
                                        "memBufLimit": {
                                            "type": "string"
                                        },
                                        "skipLongLines": {
                                            "type": "string"
                                        },
                                        "refreshInterval": {
                                            "type": "integer"
                                        },
                                        "rotateWait": {
                                            "type": "integer"
                                        },
                                        "storageType": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        },
                        "hostLog": {
                            "type": "object",
                            "properties": {
                                "dmesg": {
                                    "type": "object",
                                    "properties": {
                                        "tag": {
                                            "type": "string"
                                        },
                                        "path": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "messages": {
                                    "type": "object",
                                    "properties": {
                                        "tag": {
                                            "type": "string"
                                        },
                                        "path": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "secure": {
                                    "type": "object",
                                    "properties": {
                                        "tag": {
                                            "type": "string"
                                        },
                                        "path": {
                                            "type": "string"
                                        },
                                        "db": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "parser": {
                                    "type": "string"
                                },
                                "memBufLimit": {
                                    "type": "string"
                                },
                                "skipLongLines": {
                                    "type": "string"
                                },
                                "refreshInterval": {
                                    "type": "integer"
                                }
                            }
                        }
                    }
                },
                "kubernetesFilter": {
                    "type": "object",
                    "properties": {
                        "match": {
                            "type": "string"
                        },
                        "kubeURL": {
                            "type": "string"
                        },
                        "kubeTagPrefix": {
                            "type": "string"
                        },
                        "mergeLog": {
                            "type": "string"
                        },
                        "mergeLogKey": {
                            "type": "string"
                        },
                        "k8sLoggingParser": {
                            "type": "string"
                        },
                        "k8sLoggingExclude": {
                            "type": "string"
                        },
                        "labels": {
                            "type": "string"
                        },
                        "annotations": {
                            "type": "string"
                        }
                    }
                },
                "resources": {
                    "type": "object",
                    "properties": {
                        "limits": {
                            "type": "object",
                            "properties": {
                                "memory": {
                                    "type": "string"
                                }
                            }
                        },
                        "requests": {
                            "type": "object",
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "volumeMounts": {
                    "type": "array",
                    "items": {
                        "anyOf": [
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "mountPath": {
                                        "type": "string"
                                    }
                                }
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "mountPath": {
                                        "type": "string"
                                    },
                                    "readOnly": {
                                        "type": "boolean"
                                    }
                                }
                            }
                        ]
                    }
                },
                "volumes": {
                    "type": "array",
                    "items": {
                        "anyOf": [
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "hostPath": {
                                        "type": "object",
                                        "properties": {
                                            "path": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "configMap": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        ]
                    }
                },
                "tolerations": {
                    "type": "array",
                    "items": {
                        "anyOf": [
                            {
                                "type": "object",
                                "properties": {
                                    "key": {
                                        "type": "string"
                                    },
                                    "operator": {
                                        "type": "string"
                                    },
                                    "effect": {
                                        "type": "string"
                                    }
                                }
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "operator": {
                                        "type": "string"
                                    },
                                    "effect": {
                                        "type": "string"
                                    }
                                }
                            }
                        ]
                    }
                },
                "env": {
                    "type": "object",
                    "properties": {
                        "httpServer": {
                            "type": "boolean"
                        },
                        "httpPort": {
                            "type": "string"
                        },
                        "readFromHead": {
                            "type": "string"
                        },
                        "readFromTail": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}